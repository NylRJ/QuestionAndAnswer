    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Flutter: Attach to Device",
            "type": "dart",
            "request": "attach"
        },
        {
            "name": "All Test",
            "request": "launch",
            "type": "dart",
            "program": "test/"
        },
        {
            "name": "Flutter Pixel",
            "type": "dart",
            "request": "launch",
            "program": "lib/main/main.dart"
        },
    ]
}
